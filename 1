import numpy as np

student_scores = np.array([[70, 85, 78, 90],
                           [88, 76, 92, 80],
                           [90, 91, 85, 87],
                           [75, 80, 88, 82]])

avg_scores = np.mean(student_scores, axis=0)
highest_index = np.argmax(avg_scores)
subjects = ['Math', 'Science', 'English', 'History']

print("Subject Averages:", avg_scores)
print("Highest Average Subject:", subjects[highest_index])
